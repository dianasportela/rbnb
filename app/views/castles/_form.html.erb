<%= simple_form_for(@castle) do |f| %>
  <%= f.input :castle_name %>
  <%= f.input :location %>
  <%= f.input :overview %>
  <%= f.input :castle_avg_rating %>
  <%= f.input :min_price %>
  <%= f.input :availability %>

  <%= f.input :photos, as: :file, input_html: { multiple: true } %>
  <%= f.submit %>
<% end %>


