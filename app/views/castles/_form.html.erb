<%= form_with model: @castle do |f| %>
  <%= f.label :castle_name %>
  <%= f.text_field :castle_name %>
  <%= f.label :location %>
  <%= f.text_field :location %>
  <%= f.label :castle_picture %>
  <%= f.url_field :castle_picture %>
  <%= f.label :overview %>
  <%= f.text_field :overview %>
  <%= f.submit %>
<% end %>
